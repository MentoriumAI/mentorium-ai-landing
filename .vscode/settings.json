    "titleBar.activeBackground": "#167367",
    "titleBar.inactiveBackground": "#167367cb"
  },
  "workbench.colorTheme": "SynthWave84",
  "workbench.preferredDarkColorTheme": "SynthWave84",
  "files.exclude": {
